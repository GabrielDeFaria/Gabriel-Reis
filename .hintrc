{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off",
        "frame-title": "off",
        "document-title": "off"
      }
    ],
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "meta-viewport": "off",
    "no-inline-styles": "off",
    "axe/structure": [
      "default",
      {
        "list": "off",
        "listitem": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "scroll-behavior",
          "aspect-ratio"
        ]
      }
    ],
    "disown-opener": "off"
  }
}